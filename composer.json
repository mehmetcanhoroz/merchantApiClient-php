{
  "name": "papara/papara",
  "description": "Papara PHP Client Library",
  "type": "library",
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Papara",
      "email": "papara@crosstech.com.tr"
    },
    {
      "name": "Crosstech",
      "email": "info@crosstech.com.tr"
    }
  ],
  "autoload": {
    "psr-4": {
      "Papara\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Papara\\": [
        "tests/",
        "tests/Papara/"
      ]
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "phpunit/phpunit": "^9.3"
  }
}
